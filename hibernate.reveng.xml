<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-schema="juego" match-name=".*"/>
  <table-filter match-catalog="juego" match-name="armas"/>
  <table-filter match-catalog="juego" match-name="enemigo_pantalla"/>
  <table-filter match-catalog="juego" match-name="enemigos"/>
  <table-filter match-catalog="juego" match-name="pantallas"/>
  <table-filter match-catalog="juego" match-name="personaje_pantalla"/>
  <table-filter match-catalog="juego" match-name="personajes"/>
  <table-filter match-catalog="juego" match-name="trofeos"/>
</hibernate-reverse-engineering>